     //Extra Consonants\\
    //******************\\

phoneme ts
  import_phoneme consonants/ts
endphoneme

phoneme dz
  import_phoneme consonants/dz
endphoneme

phoneme p_h
  import_phoneme consonants/ph
endphoneme

phoneme k_h
  import_phoneme consonants/kh
endphoneme

      //********\\
     //Diacritics\\
    //************\\
    
phoneme "               //Primary stress
  import_phoneme base/'
endphoneme

phoneme %               //Secondary stress
  import_phoneme base/\,
endphoneme

phoneme :               //Lengthen
  import_phoneme base/:
endphoneme

phoneme =               //Syllabic consonant
  import_phoneme base/-
endphoneme

phonemetable hi base
include ph_hindi
phonemetable bg base
include ph_bulgarian