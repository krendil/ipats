//Replace small caps with regular capitals to make things less confusing
.replace ʙ B
.replace ɢ G
.replace ɡ g
.replace ʜ H
.replace ɪ I
.replace ʟ L
.replace ɴ N
.replace ʀ R
.replace ʏ Y

      //****\\
     //Vowels\\
    //********\\

.group  a
        a   a

.group  ɐ
        ɐ   6

.group  ɑ
        ɑ   A

.group  ɒ
        ɒ   Q

.group  æ
        æ   {

.group  e
        e   e

.group  ə
        ə   @

.group  ɘ
        ɘ   @\

.group  ɛ
        ɛ   E

.group  ɜ
        ɜ   3

.group  ɤ
        ɤ   7

.group  i
        i   i
        
.group  ɨ
        ɨ   1
        
.group  I
        I   I

.group  o
        o   o

.group  ɵ
        ɵ   8

.group  ø
        ø   2

.group  œ
        œ   9

.group  ɶ
        ɶ   &

.group  ɔ
        ɔ   0

.group  u
        u   u

.group  ʉ
        ʉ   }

.group  ʊ
        ʊ   U

.group  ʌ
        ʌ   V

.group  y
        y   y


