
      //****\\
     //Vowels\\
    //********\\

phoneme a
  vowel starttype #a endtype #a
  ipa a
  length 150
  FMT(vowel/a_5)
endphoneme

phoneme 6
  vowel  starttype #@  endtype #@
  ipa ɐ
  unstressed
  length 150
  FMT(vowel/a#_3)
endphoneme

phoneme A
  vowel  starttype #a  endtype #a
  ipa ɑ
  length 150
  FMT(vowel/aa_2)
endphoneme

phoneme Q
  vowel  starttype #o  endtype #o
  ipa ɒ
  length 150
  FMT(vowel/oo_en)
endphoneme

phoneme {
  vowel  starttype #a  endtype #a
  ipa æ
  length 150
  FMT(vowel/&)
endphoneme

phoneme e
  vowel  starttype #i  endtype #i
  ipa e
  length 150
  FMT(vowel/e)
endphoneme

phoneme @    //  Schwa
  vowel   starttype #@  endtype #@
  ipa ə
  unstressed
  length 150
  FMT(vowel/@)
endphoneme

phoneme @\
  vowel   starttype #@ endtype #@
  ipa ɘ
  length 150
  FMT(vowel/@_2)
endphoneme

